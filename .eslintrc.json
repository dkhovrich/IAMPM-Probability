{
    "globals": {
        "document": true
    },
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "linebreak-style": 0,
        "react/jsx-indent": [
            4,
            4
        ]
    }
}