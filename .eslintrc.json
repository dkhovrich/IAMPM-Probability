{
    "globals": {
        "document": true
    },
    "extends": "airbnb",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": 0,
        "comma-dangle": [
            "error",
            "never"
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/jsx-indent": [
            4,
            4
        ],
        "react/jsx-indent-props": [
            4,
            4
        ],
        "jsx-a11y/label-has-for": 0
    }
}